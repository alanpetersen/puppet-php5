{
  "name": "alanpetersen-php5",
  "version": "0.1.0",
  "author": "alanpetersen",
  "summary": "installs PHP 5.5 on RedHat/CentOS 6 machines",
  "license": "Apache-2.0",
  "source": "https://github.com/alanpetersen/puppet-php5.git",
  "project_page": "https://github.com/alanpetersen/puppet-php5",
  "issues_url": "https://github.com/alanpetersen/puppet-php5/issues",
  "dependencies": [
    {"name":"nanliu/staging","version_requirement":">= 1.0.1 < 2.0.0"}
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.0.0 < 2015.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6"
      ]
    },
    {
